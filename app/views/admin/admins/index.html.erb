<h1> Listing admins </h1>
<%= link_to 'Show users', users_path %>
<%= link_to 'Create admin', admin_sign_up_path %>
<table>
	<tr>
		<th>Email</th>
	</tr>

	<% @admins.each do |admin| %>
		<tr>
			<td><%= admin.email %></td>
 			<td> <%= button_to 'Delete', admin_admin_path(admin), method: :delete, confirm: 'Are you shure?', class: 'btn btn-danger' %>
			<td><%= link_to 'Update admin', edit_admin_admin_path(admin) %></td>
		</tr>
	<% end %>
</table>
<%= link_to 'Back', admin_admin_path(current_admin) %>